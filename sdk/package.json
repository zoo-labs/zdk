{
    "name": "@zoolabs/sdk",
    "version": "5.4.3",
    "description": "An SDK that can be used in any javascript/typescript context to easily interact with Zoo liquidity APIs",
    "source": "src/index.ts",
    "module": "dist/index.mjs",
    "type": "module",
    "types": "dist/index.d.ts",
    "author": "Zoo Labs Foundation",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p .",
        "clean": "rm -rf dist",
        "version": "yarn version",
        "version:package": "sh ../../scripts/package-version.sh",
        "version:update": "yarn version ${0}; PACKAGE_VERSION=$(yarn version:package); git add -A; git commit -m \"ðŸŽ‰ Release client package v$PACKAGE_VERSION\"; git push",
        "syncApi": "node ./sync-api.mjs",
        "changelog": "node ../../scripts/generate-changelog.js package=sdk"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zoo-labs/zdk"
    },
    "sideEffects": false,
    "keywords": [
        "nft",
        "zoo",
        "zoo-sdk",
        "zdk",
        "protocol",
        "sdk"
    ],
    "peerDependencies": {
        "ethers": "^5.7.2"
    },
    "dependencies": {
        "@types/node": "^18.14.0",
        "axios": "^1.3.3",
        "typescript": "^4.9.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "openapi-typescript": "^6.1.0"
    }
}
